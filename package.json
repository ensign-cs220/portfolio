{
    "name": "portfolio",
    "version": "1.0.0",
    "description": "My portfolio",
    "scripts": {
        "lint": "npx eslint ./",
        "format": "npx prettier --write .",
        "watch": "npx tailwindcss -i ./src/app.css -o ./dist/output.css --watch",
        "build-css": "npx tailwindcss -i ./src/app.css -o ./dist/output.css -m",
        "build-js": "npx esbuild ./src/app.js --outfile=./dist/output.js --sourcemap --bundle --minify"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ensign-cs220/portfolio.git"
    },
    "author": "",
    "license": "MIT license",
    "bugs": {
        "url": "https://github.com/ensign-cs220/portfolio.git/issues"
    },
    "homepage": "https://github.com/ensign-cs220/portfolio.git#readme",
    "dependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@tailwindcss/typography": "^0.5.7",
        "alpinejs": "^3.10.5",
        "esbuild": "^0.16.10",
        "tailwindcss": "^3.2.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "eslint": "^8.30.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.2",
        "prettier": "^2.8.1"
    }
}
